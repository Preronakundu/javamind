1234
123
12
1

class Main
{
    public static void main (String[] args){
        
        int s=4;
        for(int i=s;i>=1;i--){
            for(int j=1;j<=i;j++){
                System.out.print(j);
                
            }
            
            System.out.print("\n");
        }
    }
}
